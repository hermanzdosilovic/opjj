<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0" metadata-complete="false">

	<display-name>Moja prva aplikacija</display-name>
	<description>
     	Prva aplikacija
  	</description>

	<servlet>
		<servlet-name>s1</servlet-name>
		<servlet-class>hr.fer.zemris.java.tecaj_12.servleti.PrviServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>s2</servlet-name>
		<servlet-class>hr.fer.zemris.java.tecaj_12.servleti.SlikaServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>s1</servlet-name>
		<url-pattern>/prvi</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>s1</servlet-name>
		<url-pattern>/index.html</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>s2</servlet-name>
		<url-pattern>/slika</url-pattern>
	</servlet-mapping>

</web-app>